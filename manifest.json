{
    "manifest_version": 2,
    "name": "tablist",
    "version": "0",

    "description": "Delete and export your tabs",

    "icons": {
	"48": "icons/tablist-48.png"
    },

    "permissions":
    [
	"tabs",
        "storage",
        "activeTab"
    ],

    "browser_action": {
	"default_icon": "icons/tablist-48.png",
	"default_title": "tablist",
    "default_popup": "popup/tablist.html",
    "browser_style": true
    }
}
